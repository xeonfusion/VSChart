{
  "name": "vschart",
  "version": "0.3.0",
  "author": {
    "name": "John George K. (xeonfusion@yahoo.com)"
  },
  "description": "React electron app for digital anaesthesia charting",
  "build": {
    "appId": "com.example.vschart_electron_app",
    "productName": "VSChart",
    "copyright": "Copyright Â© 2020-21 John George K. (xeonfusion@yahoo.com)"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "browser-fs-access": "^0.14.2",
    "chart.js": "^2.9.4",
    "interactjs": "^1.10.3",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-calendar-timeline": "^0.27.0",
    "react-dom": "^16.14.0",
    "react-google-charts": "^3.0.15",
    "react-numpad": "^5.1.3",
    "react-scripts": "3.4.1",
    "wait-on": "^5.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "json-server": "json-server --watch db.json --port 5000",
    "devserver": "concurrently \"npm start\" \"npm run json-server\"",
    "build": "react-scripts build && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron": "concurrently \"npm run devserver\" \"wait-on http://localhost:3000/ && npm run electron-dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^9.4.0",
    "electron-builder": "^22.9.1",
    "json-server": "^0.16.3",
    "typescript": "^3.9.9"
  }
}
