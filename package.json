  },
  "description": "React electron app for digital anaesthesia charting",
  "build": {
    "appId": "com.example.vschart_electron_app",
    "productName": "VSChart",
    "copyright": "Copyright Â© 2020-22 John George K. (xeonfusion@yahoo.com)"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "browser-fs-access": "^0.24.0",
    "chart.js": "^3.7.0",
    "chartjs-adapter-moment": "^1.0.0",
    "interactjs": "^1.10.11",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-calendar-timeline": "^0.27.0",
    "react-dom": "^17.0.2",
    "react-google-charts": "^4.0.0",
    "react-numpad": "^5.1.3",
    "react-scripts": "^5.0.0",
    "react-swipeable-views": "^0.14.0",
    "react-to-print": "^2.14.4",
    "wait-on": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "json-server": "json-server --watch db.json --port 5000",
    "devserver": "concurrently \"npm start\" \"npm run json-server\"",
    "build": "react-scripts build && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron": "concurrently \"npm run devserver\" \"wait-on http://localhost:3000/ && npm run electron-dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "json-server": "^0.17.0",
    "mqtt": "^4.3.4",
    "typescript": "^4.5.5",
    "url": "^0.11.0"
  }
